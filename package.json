{
  "name": "json-remap-engine",
  "version": "0.3.0",
  "description": "JSONPath-driven remapping engine that produces JSON Patch operations.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "docs",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "check": "npm run lint && npm run test",
    "prepublishOnly": "npm run check && npm run build"
  },
  "keywords": [
    "json",
    "jsonpath",
    "jsonpatch",
    "transform",
    "rules engine"
  ],
  "author": "Rahul Powar",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulpowar/json-remap-engine.git"
  },
  "bugs": {
    "url": "https://github.com/rahulpowar/json-remap-engine/issues"
  },
  "homepage": "https://github.com/rahulpowar/json-remap-engine#readme",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "jsonpath-plus": "^10.3.0",
    "@byjohann/toon": "^0.5.1"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
